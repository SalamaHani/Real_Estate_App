generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type ListingAgents {
  broker_email             String
  broker_phone             String
  brokerage_name           String
  email                    String
  first_name               String
  franchise_name           String
  full_name                String
  last_name                String
  mobile_phone_line_number String
  office_city              String
  office_line_number       String
  office_name              String
  office_state             String
  office_street_address    String
  office_zip_code          String
  photo                    String
  social_media             ListingAgentsSocialMedia
}

type ListingAgentsSocialMedia {
  facebook  String?
  instagram String?
  linkedin  String?
  pinterest String?
  twitter   String?
  youtube   String?
}

type ListingLocation {
  city            String
  county          String
  display_address Boolean?
  lat             Float
  lng             Float
  neighborhood    String
  state           String
  street_address  String
  unit_number     String
  zip_code        String
}

type ListingOffices {
  broker_email          String
  broker_phone          String
  brokerage_name        String
  franchise_name        String
  office_city           String
  office_name           String
  office_state          String
  office_street_address String
  office_zip_code       String
}

type SeavdsearchuserQury {
  Bads   String
  Baths  String
  Status String
  city   String
}

model Areacuntry {
  id               String   @id @default(auto()) @map("_id") @db.ObjectId
  AboutAllCountry  BigInt
  AvgWorkCommute   BigInt
  Catholic         BigInt
  Maps             String
  MedianAge        BigInt
  MedianAreaIncome BigInt
  MedianSalePrice  BigInt
  Mosques          BigInt
  Stores           BigInt
  Universities     BigInt
  areaKm2          Float
  description      String
  hospitalsCount   BigInt
  living_area      BigInt
  name             String
  photos           String[]
  population       BigInt
  property_type    String
  schoolsCount     BigInt
}

model Seavdsearchuser {
  id              String              @id @default(auto()) @map("_id") @db.ObjectId
  createdAt       DateTime            @db.Date
  email_frequency String
  nameSearch      String
  qury            SeavdsearchuserQury
  updatedAt       DateTime            @db.Date
  url             String
  userId          String
}

model account {
  id                   String    @id @map("_id")
  accessToken          String?
  accessTokenExpiresAt DateTime? @db.Date
  accountId            String
  createdAt            DateTime  @db.Date
  idToken              String?
  password             String?
  providerId           String
  scope                String?
  updatedAt            DateTime  @db.Date
  userId               String
}

model favorite {
   id          String @id @default(auto()) @map("_id") @db.ObjectId
  userId     String     // reference to User model
  listingId  String     @db.ObjectId
  listing    listing    @relation(fields: [listingId], references: [id])
  createdAt  DateTime   @default(now())
  // id        String @id @default(auto()) @map("_id") @db.ObjectId
  // listingId String @db.ObjectId
  // userId    String
}

model interest {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  agentemail String
  createdAt  DateTime @db.Date
  email      String
  listingId  String?  @db.ObjectId
  name       String
  userId     String
}

model listing {
  id                      String           @id @default(auto()) @map("_id") @db.ObjectId
  favorites  favorite[]  @relation
  agents                  ListingAgents[]
  always_email_agents     String
  bathrooms               BigInt
  bedrooms                BigInt
  caravan_end_time        String?
  caravan_food_offered    String?
  caravan_notes           String?
  caravan_start_date      String?
  caravan_start_time      String?
  coordinates             Float[]
  description             String
  full_bathrooms          BigInt
  half_bathrooms          BigInt
  is_caravan              Boolean?
  is_rental               Boolean
  listing_email           String
  listing_source          String
  listing_status          String
  living_area             BigInt?
  location                ListingLocation
  lot_size                Float?
  make_notified           Boolean
  mls_name                String
  office_ids              String[]
  offices                 ListingOffices[]
  photos                  String[]
  price                   Float
  property_type           String
  schools_district        String
  single_property_website String
  url                     String
  virtual_tour_url        String
  year_built              BigInt?
}

model review {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  authorName String
  comment    String
  createdAt  DateTime @db.Date
  listingId  String   @db.ObjectId
  rating     BigInt
  updatedAt  DateTime @db.Date
  userId     String
}

model session {
  id        String   @id @map("_id")
  createdAt DateTime @db.Date
  expiresAt DateTime @db.Date
  ipAddress String
  token     String   @unique
  updatedAt DateTime @db.Date
  userAgent String
  userId    String
}

model user {
  id            String   @id @map("_id")
  createdAt     DateTime @db.Date
  email         String   @unique
  emailVerified Boolean
  name          String
  updatedAt     DateTime @db.Date
}

model verification {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
